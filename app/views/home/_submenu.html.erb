<div class="row row-submenu" data-modulo-id="<%= submenu[:id] %>">
  <div class="container-submenu">
    <% inicio = 0 %>
    <% tamanho = 10 %>
    <div class="row">
      <% 3.times do %>
      <div class="col-md-4">
        <ul class="nav nav-pills nav-stacked">
          <% array = submenu[:submenu].slice(inicio, tamanho) %>

          <%= render partial: "funcionalidades", locals: { submenu: array } %>
        </ul>
        <% inicio += tamanho %>
      </div>
      <% end -%>
    </div>
  </div>
</div>