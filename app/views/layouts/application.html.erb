                <a href="#"><i class="fa fa-bell"></i> <span class="badge">42</span></a>
              </li> -->

              <li class="dropdown">
                <div class="dropdown-toggle dropdown-user" id="dropdownMenu1" data-toggle="dropdown">
                  <%#= @usuario_logado.nome %>
                  <span class="caret"></span>
                </div>
                <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                  <li>
                    <%#= link_to session_path, method: :delete do %>
                      <!-- <i class="fa fa-sign-out"></i> Sair -->
                    <%# end %>
                  </li>
                </ul>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </nav>

    <div class="container-fluid container-main">
      <div class="row">
        <div class="col-md-12 main">
          <div class="container">
            <%= yield %>
          </div>
        </div>
      </div>
    </div> <!-- /container -->
    <!--%= render "shared/footer" %-->
   </div>
   <script type="text/javascript">
